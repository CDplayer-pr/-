(pageNum) {\r\n      if (pageNum) this.activityPageNum = pageNum\r\n      this.$request.get('/activity/selectPage2', {\r\n        params: {\r\n          pageNum: this.activityPageNum,\r\n          pageSize: this.activityPageSize,\r\n        }\r\n      }).then(res => {\r\n        if (res.code === '200') {\r\n          this.activityData = res.data?.list\r\n          this.activityTotal = res.data?.total\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n\r\n      })\r\n    },\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n.el-col-5{\r\n  width: 20%;\r\n  max-width: 20%;\r\n  padding: 10px 10px;\r\n}\r\n</style>"],"mappings":"AAmEA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,QAAA,GACAC,OAAA,6BACAA,OAAA,6BACAA,OAAA,4BACA;MACAC,cAAA;MACAC,YAAA;MACAC,eAAA;MACAC,gBAAA;MACAC,aAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,cAAA;IACA,KAAAC,YAAA;EACA;EACA;EACAC,OAAA;IACAF,eAAA;MACA,KAAAG,QAAA,CAAAC,GAAA,0BAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAb,cAAA,GAAAY,GAAA,CAAAf,IAAA;QACA;UACA,KAAAiB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAC,MAAAC,GAAA;MACAC,QAAA,CAAAC,IAAA,GAAAF,GAAA;IACA;IACAX,aAAAc,OAAA;MACA,IAAAA,OAAA,OAAAnB,eAAA,GAAAmB,OAAA;MACA,KAAAZ,QAAA,CAAAC,GAAA;QACAY,MAAA;UACAD,OAAA,OAAAnB,eAAA;UACAqB,QAAA,OAAApB;QACA;MACA,GAAAQ,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAZ,YAAA,GAAAW,GAAA,CAAAf,IAAA,EAAA2B,IAAA;UACA,KAAApB,aAAA,GAAAQ,GAAA,CAAAf,IAAA,EAAA4B,KAAA;QACA;UACA,KAAAX,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MAEA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}